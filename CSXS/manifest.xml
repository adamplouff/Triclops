<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.triclops" ExtensionBundleVersion="1.0.0" ExtensionBundleName="triclops" Version="8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.triclops.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="ILST" Version="23" />
      <Host Name="AEFT" Version="16" />
      <Host Name="PHXS" Version="20" />
      <Host Name="PPRO" Version="13" />
      <Host Name="IDSN" Version="14" />
      <Host Name="AUDT" Version="12" />
      <Host Name="FLPR" Version="18" /></HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="8.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.triclops.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./public/index-dev.html</MainPath>
          <!-- <MainPath>./dist/index.html</MainPath> -->
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Triclops</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>280</Width>
            </Size>
            <MaxSize>
              <Height>1000</Height>
              <Width>1000</Width>
            </MaxSize>
            <MinSize>
              <Height>300</Height>
              <Width>260</Width>
            </MinSize>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
